-- [beforeClass]
DROP TABLE IF EXISTS DC_NEWS;

CREATE TABLE DC_NEWS (
  ID INT(11) UNSIGNED NOT NULL AUTO_INCREMENT,
  PIC VARCHAR(100),
  TITLE VARCHAR(200),
  TOP TINYINT(4),
  CONTENT TEXT,
  UPDATE_TIME DATE ,
  SOURCE VARCHAR(64),
  PRIMARY KEY (ID)
) ENGINE=InnoDB DEFAULT CHARSET=UTF8;

